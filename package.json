{
  "name": "@canisminor/colors",
  "version": "1.0.0",
  "description": "Color system for canisminor",
  "main": "src/index.ts",
  "repository": "git@github.com:canisminor1990/canisminor-colors.git",
  "author": "CanisMinor <i@canisminor.cc>",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prepublishOnly": "npm run build",
    "---------": "",
    "release": "semantic-release",
    "--------- ": "",
    "test": "yarn lint",
    "_test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "---------  ": "",
    "lint": "eslint --cache --quiet ./src --ext .ts,.tsx,.js --format=pretty && yarn type-check",
    "lint:fix": "eslint --fix --cache --quiet --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint-staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "---------   ": "",
    "clean": "rm -rf dist",
    "reinstall": "yarn clean && rm -rf node_modules && yarn -W"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "yarn lint:fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "react": "^16.12.0 || ^17.0.0"
  },
  "devDependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "chroma-js": "^2.4.2",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0",
    "husky": "^7.0.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "semantic-release": "^19.0.2",
    "semantic-release-config-gitmoji": "^1.2.4",
    "commitlint": "^16.2.3",
    "commitlint-config-gitmoji": "^2.2.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
